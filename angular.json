{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-timer": {
      "root": "projects/ng-timer",
      "sourceRoot": "projects/ng-timer/src",
      "projectType": "library",
      "prefix": "ng",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-timer/tsconfig.lib.json",
            "project": "projects/ng-timer/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/ng-timer/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-timer/src/test.ts",
            "tsConfig": "projects/ng-timer/tsconfig.spec.json",
            "karmaConfig": "projects/ng-timer/karma.conf.js",
            "watch": false
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-timer/tsconfig.lib.json",
              "projects/ng-timer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-timer-wrapper": {
      "root": "projects/ng-timer-wrapper/",
      "sourceRoot": "projects/ng-timer-wrapper/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-timer-wrapper",
            "index": "projects/ng-timer-wrapper/src/index.html",
            "main": "projects/ng-timer-wrapper/src/main.ts",
            "polyfills": "projects/ng-timer-wrapper/src/polyfills.ts",
            "tsConfig": "projects/ng-timer-wrapper/tsconfig.app.json",
            "assets": [
              "projects/ng-timer-wrapper/src/favicon.ico",
              "projects/ng-timer-wrapper/src/assets"
            ],
            "styles": [
              "projects/ng-timer-wrapper/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ng-timer-wrapper/src/environments/environment.ts",
                  "with": "projects/ng-timer-wrapper/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-timer-wrapper:build",
            "port": 5001,
            "open": true
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-timer-wrapper:build:production",
              "port": 5002
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-timer-wrapper:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-timer-wrapper/src/test.ts",
            "polyfills": "projects/ng-timer-wrapper/src/polyfills.ts",
            "tsConfig": "projects/ng-timer-wrapper/tsconfig.spec.json",
            "karmaConfig": "projects/ng-timer-wrapper/karma.conf.js",
            "styles": [
              "projects/ng-timer-wrapper/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/ng-timer-wrapper/src/favicon.ico",
              "projects/ng-timer-wrapper/src/assets"
            ],
            "watch": false
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-timer-wrapper/tsconfig.app.json",
              "projects/ng-timer-wrapper/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-timer-wrapper-e2e": {
      "root": "projects/ng-timer-wrapper-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ng-timer-wrapper-e2e/protractor.conf.js",
            "devServerTarget": "ng-timer-wrapper:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-timer-wrapper:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/ng-timer-wrapper-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-timer-wrapper"
}